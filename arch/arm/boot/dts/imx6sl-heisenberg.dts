#include "imx6sl.dtsi"

/ {
	model = "Heisenberg P0";
	compatible = "amzn,heisenberg", "fsl,imx6sl";

	memory {
		reg = <0x80000000 0x20000000>;
	};

	wifi_brcm {
		compatible = "bcm,bcm43430";
		wl_reg_on = <&gpio2 15 0>; /* Power on/off GPIO */
		wl_host_wake = <&gpio3 31 0>; /* Host wake for OOB */
	};

	bt_pwr_ctrl {
		compatible = "amzn,bt_pwr_ctrl";
		bt_rst = <&gpio2 19 0>; /* Power on/off GPIO */
		bt_dev_wake = <&gpio5 10 0>; /*BT wake for OOB */
		bt_host_wake = <&gpio3 29 0>; /* Host wake for OOB */
	};

	heisenberg_hall {
		compatible = "amzn,heisenberg_hall";
		hall_int_n= <&gpio4 16 0>; /*hall detect GPIO */
		status = "okay";		
	};

	imx_ion {
		compatible = "fsl,mxc-ion";
		fsl,heap-id = <0>;
		fsl,heap-cacheable = <1>;
	};

	regulator-virtuals {
		compatible = "simple-bus";

		virt-buck1 {
			compatible = "regulator-virtual";
			virtual-supply = "buck1";
		};
		virt-buck2 {
			compatible = "regulator-virtual";
			virtual-supply = "buck2";
		};
		virt-buck3 {
			compatible = "regulator-virtual";
			virtual-supply = "buck3";
		};
		virt-buck4 {
			compatible = "regulator-virtual";
			virtual-supply = "buck4";
		};
		virt-buck5 {
			compatible = "regulator-virtual";
			virtual-supply = "buck5";
		};
		virt-ldo1 {
			compatible = "regulator-virtual";
			virtual-supply = "ldo1";
		};
		virt-ldo2 {
			compatible = "regulator-virtual";
			virtual-supply = "ldo2";
		};
		virt-ldo3 {
			compatible = "regulator-virtual";
			virtual-supply = "ldo3";
		};
		virt-ldo4 {
			compatible = "regulator-virtual";
			virtual-supply = "ldo4";
		};
		virt-ldo5 {
			compatible = "regulator-virtual";
			virtual-supply = "ldo5";
		};
		user-dvref {
			compatible = "userspace_consumer";
			uc-name = "dvref";
			suck-supply = "dvref";
		};
		user-lpsr {
			compatible = "userspace_consumer";
			uc-name = "lpsr";
			suck-supply = "lpsr";
		};
		virt-wled {
			compatible = "regulator-virtual";
			virtual-supply = "wled";
		};
	};
};

&usbotg1 {
	vbus-supply = <&ldo3_reg>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg1_1>;
	disable-over-current;
	imx6-usb-charger-detection;
	status = "okay";
};



&epdc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_epdc_0>;
	status = "okay";
};


&kpp {
        pinctrl-names = "default";
        pinctrl-0 = <&pinctrl_kpp_1>;
	status = "okay";
};


&uart2 {
	status = "disabled";
};


&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1_1>;
	status = "okay";
};

&uart3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart3_1>;
	fsl,uart-has-rtscts;
	status = "okay";
};

&uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart4_1>;
	status = "okay";
};

&usdhc1 { /*wifi*/
	pinctrl-names = "default", "state_100mhz", "state_200mhz";
	pinctrl-0 = <&pinctrl_usdhc1_1>;
	pinctrl-1 = <&pinctrl_usdhc1_1_100mhz>;
	pinctrl-2 = <&pinctrl_usdhc1_1_200mhz>;
	keep-power-in-suspend;
	non-removable;
	/*enable-sdio-wakeup;*/
	bus-width = <4>;
	fsl,delay-line;
	status = "okay";
};

&usdhc2 { /*emmc*/
	pinctrl-names = "default", "state_100mhz", "state_200mhz";
	pinctrl-0 = <&pinctrl_usdhc2_1>;
	pinctrl-1 = <&pinctrl_usdhc2_1_100mhz>;
	pinctrl-2 = <&pinctrl_usdhc2_1_200mhz>;
	non-removable;
	keep-power-in-suspend;
	/*enable-sdio-wakeup;*/
	bus-width = <8>;
	fsl,delay-line;
	fsl,sig-voltage-switch-disable;
	fsl,sdr104-support-override;
	fsl,ddr50-support-override;
	fsl,sdr50-support-override;
	status = "okay";
};
