  #address-cells = <1>;
  #size-cells = <0>;

  cpu0: cpu@0 {
   compatible = "arm,cortex-a9";
   device_type = "cpu";
   reg = <0x0>;
   next-level-cache = <&L2>;
   operating-points = <

    996000 1275000
    396000 975000
   >;
   fsl,soc-operating-points = <

    996000 1225000
    396000 1175000
   >;
   clock-latency = <61036>;
   clocks = <&clks 95>, <&clks 20>,
     <&clks 29>, <&clks 30>,
     <&clks 3>;
   clock-names = "arm", "pll2_pfd2_396m", "step",
          "pll1_sw", "pll1_sys";
   arm-supply = <&reg_arm>;
   pu-supply = <&reg_pu>;
   soc-supply = <&reg_soc>;
  };
 };

 intc: interrupt-controller@00a01000 {
  compatible = "arm,cortex-a9-gic";
  #interrupt-cells = <3>;
  #address-cells = <1>;
  #size-cells = <1>;
  interrupt-controller;
  reg = <0x00a01000 0x1000>,
        <0x00a00100 0x100>;
 };

