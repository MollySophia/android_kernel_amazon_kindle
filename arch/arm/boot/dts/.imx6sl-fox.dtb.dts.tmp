# 4 "arch/arm/boot/dts/imx6sl-fox.dts" 2

/ {
 model = "Fox P0";
 compatible = "amzn,fox", "fsl,imx6sl";

 memory {
  reg = <0x80000000 0x20000000>;
 };






 gpio-keys {
  compatible = "gpio-keys";

  volume-up {
   label = "Volume Up";
   gpios = <&gpio3 27 1>;
   linux,code = <115>;
   debounce_interval = <20>;
  };

  volume-down {
   label = "Volume Down";
   gpios = <&gpio3 29 1>;
   linux,code = <114>;
   debounce_interval = <20>;
  };
  mic {
   label = "Mic";
   gpios = <&gpio3 25 1>;
   linux,code = <143>;
   debounce_interval = <20>;
   gpio-key,wakeup;
  };
  play {
   label = "Play";
   gpios = <&gpio4 3 1>;
   linux,code = <164>;
   debounce_interval = <20>;
  };
  forward {
   label = "Forward";
   gpios = <&gpio3 31 1>;
   linux,code = <159>;
   debounce_interval = <20>;
  };
  rewind {
   label = "Rewind";
   gpios = <&gpio4 1 1>;
   linux,code = <168>;
   debounce_interval = <20>;
  };
  bt-pair {
   label = "Bluetooth Pair";
   gpios = <&gpio4 5 1>;
   linux,code = <218>;
   debounce_interval = <20>;
   gpio-key,wakeup;
  };


 };

 sound {
  compatible = "fsl,imx-audio-max98090";
  model = "MAX98090-audio";
  cpu-dai = <&ssi2>;
  audio-codec = <&codec>;
  audio-routing =
   "Main SpeakerL", "RCVL",
   "Main SpeakerR", "RCVR",
   "Line Jack", "IN3",
   "Line Jack", "IN5",
   "AMIC", "IN1",
   "AMIC", "IN2";
  mux-int-port = <2>;
  mux-ext-port = <3>;
  mic-det-gpios = <&gpio4 22 1>;
 };
};

&epdc {
 status = "disabled";
};

&kpp {
 status = "disabled";
};

&usdhc1 {
 status = "disabled";
};

&uart2 {
 status = "disabled";
};

&uart4 {
